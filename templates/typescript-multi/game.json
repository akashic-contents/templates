{
	"width": 1280,
	"height": 720,
	"fps": 30,
	"main": "./script/_bootstrap.js",
	"assets": {
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"_bootstrap": {
			"type": "script",
			"path": "script/_bootstrap.js",
			"global": true
		},
		"parameterObject": {
			"type": "script",
			"path": "script/parameterObject.js",
			"global": true
		},
		"assets/images/entry.png": {
			"type": "image",
			"path": "assets/images/entry.png",
			"width": 200,
			"height": 40
		}
	},
	"environment": {
		"sandbox-runtime": "3",
		"nicolive": {
			"supportedModes": [
				"multi"
			]
		}
	},
	"globalScripts": []
}