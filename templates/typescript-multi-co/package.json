{
  "name": "typescript-multi-co",
  "private": true,
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "postinstall": "npm run build",
    "build": "rimraf ./script && tsc && akashic-cli-scan asset script",
    "update": "akashic-cli-scan asset && akashic-cli-scan globalScripts",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "start": "akashic-cli-serve -s nicolive",
    "export-zip": "rimraf game.zip && akashic-cli-export zip -o game.zip -s",
    "export-html": "rimraf game && akashic-cli-export html -o game"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@akashic/akashic-cli-export": "^1.9.21",
    "@akashic/akashic-cli-scan": "^0.17.7",
    "@akashic/akashic-cli-serve": "^1.16.19",
    "@akashic/akashic-engine": "^3.20.2",
    "@stylistic/eslint-plugin-ts": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.21.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.0.0",
    "rimraf": "^5.0.10",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@akc29/akashictool4multi": "^1.2.0"
  }
}
